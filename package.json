{
	"name": "helloworld",
	"publisher": "FILL_IN",
	"displayName": "CS782",
	"description": "DO NOT INSTALL. THIS IS AN EXPERIMENTAL EXTENSION MADE FOR A RESEARCH PROJECT.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:helloworld.read_home_dir",
		"onCommand:helloworld.web_request",
		"onCommand:helloworld.run_exe",
		"onCommand:helloworld.readwrite_clipboard",
		"onCommand:helloworld.discreet_terminal",
		"onCommand:helloworld.download_run_exe"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "helloworld.read_home_dir",
				"title": "Read home directory"
			},
			{
				"command": "helloworld.web_request",
				"title": "Make web request"
			},
			{
				"command": "helloworld.run_exe",
				"title": "Run executable"
			},
			{
				"command": "helloworld.readwrite_clipboard",
				"title": "Read/write clipboard"
			},
			{
				"command": "helloworld.discreet_terminal",
				"title": "Discreet terminal"
			},
			{
				"command": "helloworld.download_run_exe",
				"title": "Download an executable from a web URL and run it"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"clipboardy": "2.3.0",
		"node-fetch": "^2.0.0",
		"vsce": "^1.100.2"
	}
}
